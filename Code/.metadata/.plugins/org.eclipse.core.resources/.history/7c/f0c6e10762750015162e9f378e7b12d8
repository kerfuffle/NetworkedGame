package net.kerfuffle.Server;

public class Main {

	public static void main(String args[])
	{
		Server server = new Server(25585);
		while(true)
		{
			System.out.println("1");
			server.admitClients();
			System.out.println("1");
			server.updateThreads();
			System.out.println("2");
			server.run();
			System.out.println("3");
		}
	}
	
}
